{
  "name": "databox-os-monitor-driver",
  "version": "0.1.0",
  "description": "Databox driver to log local os",
  "scripts": {
    "start": "node src/main.js",
    "build": "docker build -t databox/databox-os-monitor-driver .",
    "deploy": "docker tag databox/databox-os-monitor-driver registry.upintheclouds.org/databox-os-monitor-driver && docker push registry.upintheclouds.org/databox-os-monitor-driver",
    "clean": "node ./node_modules/modclean/bin/modclean.js -p ./node_modules/ -r",
    "getshell": "docker exec -i -t databox-os-monitor-driver /bin/bash",
    "kill": "docker kill databox-os-monitor-driver",
    "attach": "docker attach databox-os-monitor-driver",
    "shell": "docker run -i -t databox/databox-os-monitor-driver /bin/bash",
    "local": "docker run -i -t databox/databox-os-monitor-driver"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/me-box/databox-os-monitor-driver.git"
  },
  "keywords": [
    "docker",
    "personal",
    "data"
  ],
  "author": "Anthony Brown <Anthony.Brown@nottingham.ac.uk> ()",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/me-box/databox-os-monitor-driver/issues"
  },
  "homepage": "https://github.com/me-box/databox-os-monitor-driver",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "intercept-stdout": "^0.1.2",
    "node-json2html": "^1.0.0",
    "os-monitor": "",
    "promise": "",
    "request": "^2.69.0",
    "modclean":""
  }
}
